import Foundation

class athleteProfile:NSObject{
    
    var hardwareID: String!
    var athleteName: String!
    var inWorkout: Bool!
    var currentHR: Float!
    var displayingOnGraph: Bool!
    var statistics: athleteAnalytics!
    var workouts: workout! //this is only one it needs to be a bunch of them
    
    override init() {
        hardwareID = "temp";
        athleteName = "temp";
        inWorkout = false;
        currentHR = 0;
        displayingOnGraph = false;
        statistics = nil;
        workouts = nil;
    }
    
}
